{
	"name": "nubesgen-vscode",
	"displayName": "NubesGen",
	"description": "Scaffold new cloud projects with NubesGen",
	"license": "MIT",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.6.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:nubesgen-vscode.generate"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "nubesgen-vscode.generate",
				"title": "NubesGen: Generate new project"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"test:ws": "rm -rf ./out/test-ws && mkdir -p ./out/test-ws"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^14.14.37",
		"@types/unzipper": "^0.10.3",
		"@types/vscode": "^1.55.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"got": "^11.8.2",
		"unzipper": "^0.10.11"
	}
}
